{
  "name": "react-router-endpoint-diff",
  "version": "0.1.0",
  "description": "CLI tool to detect API endpoint changes in React Router applications",
  "main": "dist/index.js",
  "bin": {
    "rrdiff": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "pnpm run build",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src",
    "check": "biome check --apply ./src",
    "debug": "ts-node src/cli.ts",
    "debug:verbose": "ts-node src/cli.ts --verbose --summary-only",
    "debug:help": "ts-node src/cli.ts --help",
    "start": "node dist/cli.js"
  },
  "keywords": [
    "react-router",
    "api",
    "endpoint",
    "diff",
    "cli",
    "git"
  ],
  "author": "seitaro",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "parse-diff": "^0.11.1",
    "simple-git": "^3.20.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.0",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  }
}
